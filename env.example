# Configuración del servidor
PORT=3001
NODE_ENV=development

# MongoDB
MONGODB_URI=mongodb://localhost:27017/gmaps-leads-scraper

# WhatsApp Web.js
WHATSAPP_SESSION_PATH=./whatsapp-session

# Slack Webhook (opcional)
SLACK_WEBHOOK_URL=https://hooks.slack.com/services/YOUR/SLACK/WEBHOOK

# Configuración de rotación de IPs
WG_INTERFACE=wg0
WG_PEERS_DIR=./wg/peers

# Configuración de throttling (en segundos)
MIN_DELAY=6
MAX_DELAY=8

# Configuración del bot
BOT_INTERVAL=60000
MESSAGE_SEQUENCE_DELAY=5000

# APIs de validación de teléfonos (opcional)
# NumVerify API: https://numverify.com/
NUMVERIFY_API_KEY=your_numverify_api_key_here

# Abstract API: https://www.abstractapi.com/phone-validation-api
ABSTRACT_API_KEY=your_abstract_api_key_here 